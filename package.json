{
  "name": "nash-ui",
  "version": "1.1.4",
  "description": "nash-ui",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=production webpack --config build/webpack.modules.config.js",
    "dev": "cross-env NODE_ENV=development webpack --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "serve": "webpack-dev-server --config build/webpack.dev.js",
    "lint": "eslint --ext .html,.js,.vue src example --fix",
    "husky": "husky install",
    "build1": "cross-env NODE_ENV=production node build/build.js",
    "commit": "git cz"
  },
  "keywords": [
    "ui",
    "web"
  ],
  "author": "nash 370295005@qq.com",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^7.20.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^2.2.2",
    "postcss-loader": "^7.0.1",
    "shelljs": "^0.8.4",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.52.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "better-scroll": "^2.4.2",
    "less": "^4.1.3",
    "less-loader": "^11.0.0"
  },
  "browserslist": [
    "> 1%",
    "not ie <= 11",
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint --ext .js,.html,.vue src --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  }
